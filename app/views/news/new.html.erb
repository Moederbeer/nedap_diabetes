<%= form_for(@news) do |f| %>
<%= f.label :news_title %>
<%= f.text_field :news_title %>

<%= f.label :news_author %>
<%= f.text_field :news_author %>

<%= f.label :news_content %>
<%= f.text_field :news_content %>

<%= f.label :news_url %>
<%= f.text_field :news_url %>

<%= f.submit "Publiceer artikel", class: "btn btn-primary" %>
<% end %>
